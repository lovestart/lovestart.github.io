	background-color: #ff5144;
	background-image: -webkit-linear-gradient(right, #f54, #ff7444);
	background-image: -moz-linear-gradient(right, #f54, #ff7444);
	background-image: -o-linear-gradient(right, #f54, #ff7444);
	background-image: -ms-linear-gradient(right, #f54, #ff7444);
	background-image: linear-gradient(to left, #f54, #ff7444);
	-webkit-border-radius: 44px;
	-moz-border-radius: 44px;
	border-radius: 44px;
	letter-spacing: 2px;
	-webkit-tap-highlight-color:none;
}
/*containerThree ends*/
