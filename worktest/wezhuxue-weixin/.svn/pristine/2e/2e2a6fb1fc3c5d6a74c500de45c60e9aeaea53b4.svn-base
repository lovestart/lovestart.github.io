<script type="text/javascript">
    $(function(){
    	function updateprogress() {
            $(".progressbar").each(function() {
                var fixedwidth = $(this).width();
                var width = $(this).find(".pos").width();
                var setmanual = $(this).attr("data-range");
                if('0'!=setmanual&&'0.0'!=setmanual&&'0.00'!=setmanual){
	                $(this).find(".pos").animate({
	                    width: setmanual + "%",
	                })
                }else{
                	$(this).find(".pos").animate({
	                    width: "0%",
	                })
                }
            })
        }
        var UrlData=window.location.search;
        var locationData={};//接收链接地址传输的数据
        var mainData={};//助学团详情json
        
        (UrlData==='')&&(true,locationData='nullObject') || (UrlData!='') && (true,locationData=pub.StrToJOON(UrlData));
        //判断有没有获取数据 ps:获取到了去读数据，没有获取到提示没有获取到
        (locationData === 'nullObject')&&(true,(function(){
            $('#oDiv').show();
            pub.cs('没有获取到数据！');
        })()) || (locationData!='nullObject')&&(true,function(){
            getGroupDetail();//获取帮助所有帮助提升身价的小伙伴
        }());//判断有没有获取数据 end
        
        //获取帮助提升身价小伙伴列表的方法
        function getGroupDetail(){
        	var datas ={'version':'1.0','system':'weixin','token':'','objId':'String','method':'/finance/studentStandard/status','data':''+locationData.loanId+''};
            $.ajax({
                dataType:'jsonp',
                url:pub.jsonPApiT,
                data:datas,
                jsonp:'callback',
                success:function(jsonData){
                    mainData=eval(jsonData);
					if(mainData.code=='6666'){
						if(mainData.data=='7'){
							window.location.href=pub.wxDomain+'views/zxt_stu/sb/repaymentIn.html?loanId='+locationData.loanId;
						}else{
							var datas ={'version':'1.0','system':'weixin','token':'','objId':'StudentStandardVO','method':'/finance/studentStandard/detail','@loanId':''+locationData.loanId+''};
				            $.ajax({
				                dataType:'jsonp',
				                url:pub.jsonPApiT,
				                data:datas,
				                jsonp:'callback',
				                success:function(jsonData){
				                    mainData=eval(jsonData);
									if(mainData.code=='6666'){
				                        $('#avatar').attr('src', mainData.data.basicInfo.avatar);
				                        if('A级'==mainData.data.basicInfo.levelName){
				                        	$('#levelImg').attr('src', pub.wxDomain+'views/zxt_stu/sb/images/posimgone.png');
				                        }else if('B级'==mainData.data.basicInfo.levelName){
				                        	$('#levelImg').attr('src', pub.wxDomain+'views/zxt_stu/sb/images/pospictwo.png');
				                        }else{
				                        	$('#levelImg').attr('src', pub.wxDomain+'views/zxt_stu/sb/images/pospicthree.png');
				                        }
				                        
				                        $('#userName').html(mainData.data.basicInfo.userName+'<span class="txttwo">'+mainData.data.basicInfo.loanName+'</span>');
				                        $('#schoolName').html(mainData.data.basicInfo.schoolName);
				                        $('#videoPic').attr('src',mainData.data.basicInfo.videoPicUrl);
				                        $('#video').attr('src',mainData.data.basicInfo.videoUrl);
				                        if('0.00'==mainData.data.basicInfo.schedule||'0.0'==mainData.data.basicInfo.schedule||'0'==mainData.data.basicInfo.schedule){
				                        	$('#schedule').attr('data-range','0');
				                        }else{
				                        	$('#schedule').attr('data-range',(parseFloat(mainData.data.basicInfo.schedule)*100).toFixed(1));
				                        }
				                        
				                        updateprogress();
				                        $('#interestRate').html(mainData.data.basicInfo.interestRate+"%");
				                        $('#period').html(mainData.data.basicInfo.period+"个月");
				                        $('#loanMoney').html(mainData.data.basicInfo.loanMoney+"元");
				                        $('#leftDay').html(mainData.data.basicInfo.leftDay+"天");
				                        var hrs = mainData.data.bidder;//获取投资记录
				                        if(hrs==undefined){
				                            $('#bidders').append('<li class="srt">暂无投资记录</li>');
				                            //return false;
				                        }else{
				                            for(var i=0;i<hrs.length;i++){
				                                $('#bidders').append('<li ><a href="javascript:void(0);">'+
				                                    	'<div class="imgbx"> <img src="'+hrs[i].avatar+'"> </div>'+
				                                    	'<h1>'+hrs[i].userName+'</h1>'+
				                                	'</a></li>');
				                            }
				                            if(hrs.length>0)
				                            	$('#bidders').append('<li class="arrowlink" style="float:right;"><a href="'+pub.wxDomain+'views/zxt_stu/sb/investors.html?loanId='+locationData.loanId+'"><img src="images/galrightarrow.jpg"></a></li>');
				                            else
				                            	$('#bidders').append('<li class="srt">暂无投资记录</li>');
				                        }
				                        $('#userName2').html(mainData.data.basicInfo.userName);
				                        $('#identityCard').html(mainData.data.basicInfo.identityCard);
				                        $('#schoolName2').html(mainData.data.basicInfo.schoolName);
				                        if(mainData.data.basicInfo.xyStatus=='0'){
				                        	$('#stuAuth').html("未开启校友认证");
				                        	$('#authPic').attr('src',pub.wxDomain+'views/zxt_stu/sb/images/certificationFailue.png');
				                        }else{
				                        	$('#stuAuth').html("已有"+mainData.data.basicInfo.xyNum+"名校友帮助认证");
				                        	$('#authPic').attr('src',pub.wxDomain+'views/zxt_stu/sb/images/tickfive.png');
				                        }
				                        if(mainData.data.basicInfo.zhimaStatus=='0'){
				                        	$('#zhimaFen').html("未认证");
				                        	$('#zhimaPic').attr('src',pub.wxDomain+'views/zxt_stu/sb/images/certificationFailue.png');
				                        }else{
				                        	$('#zhimaFen').html(mainData.data.basicInfo.zhimaScore+"分");
				                        	$('#zhimaPic').attr('src',pub.wxDomain+'views/zxt_stu/sb/images/tickfive.png');
				                        }
				                        $('#loanName2').html(mainData.data.basicInfo.loanName);
				                        $('#repayApproach').html(mainData.data.basicInfo.repayApproach);
				                        var reports = mainData.data.basicInfo.scorePics;
				                        if(reports==undefined){
				                        	$('#reportImg').parent().html('未上传');
				                        	$('#reportNum').html('');
				                        }else{
				                        	$('#reportImg').attr('src',reports[0]);
				                        	$('#reportNum').html('x'+reports.length);
				                        	//var slider = $('#reportPop').find('div.flexslider').data('flexslider');
				                        	for(var i=0;i<reports.length;i++){
				                            	$('#reportPop').find('ul').append('<li > <img src="'+reports[i]+'"/> </li>');
				                                //slider.addSlide('<li > <img src="'+reports[i]+'" draggable="false"/> </li>',0);
				                        	}
				                        }
				                        var awards = mainData.data.basicInfo.awardPics;
				                        if(awards==undefined){
				                        	$('#awardImg').parent().html('未上传');
				                        	$('#awardNum').html('');
				                        }else{
				                        	$('#awardPop').find('ul').html()
				                        	$('#awardImg').attr('src',awards[0]);
				                        	$('#awardNum').html('x'+awards.length);
				                        	for(var i=0;i<awards.length;i++){
				                                $('#awardPop').find('ul').append('<li > <img src="'+awards[i]+'" draggable="false"/> </li>');
				                            }
				                        }
				                        $('#leftMoney').html(mainData.data.basicInfo.leftMoney);
				                        var rules = mainData.data.interestRule.split("#");
				                        $('#rule1').html(rules[0]);
				                        $('#rule2').html(rules[1]);
				                        $('#rule3').html(rules[2]);
									}else{
										$('#oDiv').show();
				                        pub.cs(mainData.msg);
									}
				                },
				            });
						}
					}else{
						$('#oDiv').show();
                        pub.cs(mainData.msg);
					}
                }
            });
            
        };//获取帮助提升身价小伙伴列表的方法 end
        updateprogress();
        $('#helpNow').click(function(){
			//window.location.href=pub.wxDomain+'views/zxt_stu/sb/downloadApp.html';
			var datas ={'version':'1.0','system':'weixin','token':'','objId':'PayServiceVO','method':'/finance/h5/judge/authentication','@userId':''+locationData.userId+'','@type':'2'};
			$.ajax({
	            dataType:'jsonp',
	            url:pub.jsonPApiT,
	            data:datas,
	            jsonp:'callback',
	            success:function(jsonData){
	                var reData=eval(jsonData);
					if(reData.code=='6666'){
	                    if("1" == reData.data){
	                    	window.location.href=pub.wxDomain+'views/zxt_stu/sb/targetBuy.html?userId='+locationData.userId+'&loanId='+locationData.loanId+'&openId='+locationData.openId;
	                    }else{
	                    	window.location.href=pub.wxDomain+'views/zxt_stu/sb/realNameAuth.html?userId='+locationData.userId+'&loanId='+locationData.loanId+'&openId='+locationData.openId;
	                    }
					}else{
						$('#oDiv').show();
	                    pub.cs(mainData.msg);
					}
	            },
	        });
		});
	});
</script>