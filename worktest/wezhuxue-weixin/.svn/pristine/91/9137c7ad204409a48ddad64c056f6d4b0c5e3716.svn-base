@charset "utf-8";
/* CSS Document */

.body_Bg {
    background: #fafafa;
}
.containerOne .content {
    -webkit-box-shadow: -1px 0px 6px 1px rgba(220, 220, 220, 0.75);
    -moz-box-shadow: -1px 0px 6px 1px rgba(220, 220, 220, 0.75);
    box-shadow: -1px 0px 6px 1px rgba(220, 220, 220, 0.75);
    /* margin-top: 1rem; */
}
.containerOne .first {
    background: #fff;
    border-bottom: 1px solid #ddd;
    padding: .55rem 1rem;
}
.containerOne .leftImg {
    width: 2.1rem;
    padding: .2rem 0;
}
.containerOne .text {
    width: 10rem;
    padding-left: .8rem;
}
.containerOne .text a {
    color: #22cccc;
    font-size: .7rem;
    padding-bottom: .2rem;
    line-height: 1rem;
}
.containerOne .text h1 {
    color: #999;
    font-size: .6rem;
}
.containerOne .first h2 {
    color: #22cccc;
    font-size: .7rem;
	line-height:1rem;
}
.containerOne .icon {
    width: .75rem;
    padding-left: .7rem;
}
.containerOne .second {
    padding: .5rem 1rem;
    background: #fff;
}
.containerOne .secondSet {
    padding: .2rem 0;
    background: #fff;
}
.containerOne .secondSet h1 {
    color: #333;
    font-size: .7rem;
    padding-left: 1.5rem;
}
.containerOne .secondSet input {
    color: #333;
    font-size: .7rem;
    padding-left: 1.5rem;
	line-height:.9rem;
}
.containerOne .secondSet ::-webkit-input-placeholder {
    color: #333;
}
.containerOne .content_two {
    background: #fff;
    -webkit-box-shadow: -1px 0px 6px 1px rgba(220, 220, 220, 0.75);
    -moz-box-shadow: -1px 0px 6px 1px rgba(220, 220, 220, 0.75);
    box-shadow: -1px 0px 6px 1px rgba(220, 220, 220, 0.75);
    padding: .8rem .5rem;
    margin-top: 1rem;
}
.containerOne .set {
    padding-bottom: .5rem;
}
.containerOne .set h1 {
    color: #999;
    font-size: .7rem;
}
.containerOne .set h2 {
    color: #333;
    font-size: .7rem;
    padding-left: .5rem;
}
.containerOne .book {
    width: .625rem;
    padding-top: .05rem;
}
.containerOne .content_three {
    background: #fff;
    -webkit-box-shadow: -1px 0px 6px 1px rgba(220, 220, 220, 0.75);
    -moz-box-shadow: -1px 0px 6px 1px rgba(220, 220, 220, 0.75);
    box-shadow: -1px 0px 6px 1px rgba(220, 220, 220, 0.75);
    margin-top: 1rem;
}
.containerOne .content_three h1 {
    color: #333;
    font-size: .7rem;
    border-left: 3px solid #22cccc;
    border-bottom: 1px solid #ddd;
    padding: .68rem 0 .5rem .5rem;
}
.containerOne .textSet {
    padding: .6rem 0 0 .45rem;
}
.textSet h2 {
    color: #333;
    font-size: .7rem;
    padding-right: .3rem;
}
.containerOne .content_three .textSet:last-child {
    /*padding-bottom: 1.4rem;*/
    padding-bottom: 20px;
}
.containerOne .lastCont {
    padding: .5rem .6rem;
}
.containerOne .buil_icon {
    width: .75rem;
    padding-top: .15rem;
}
.containerOne .textTwo {
    padding-left: .5rem;
}
.containerOne .textTwo h2 {
    color: #333;
    font-size: .7rem;
    padding: .1rem 0 .2rem 0.25rem;
}
.containerOne .textTwo h4 {
    color: #999;
    font-size: .6rem;
}
.containerOne .lastCont h3 {
    color: #22cccc;
    font-size: .5rem;
    border: 1px solid #22cccc;
    padding: .15rem .3rem .1rem .3rem;
    border-radius: 10px;
    margin-left: .5rem;
	line-height:.5rem;
}
.containerOne .footer {
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 2.2rem;
    border-top: 1px solid #e7e7e7;
}
.containerOne .footLeft {
    width: 30%;
    background: #fff;
    height: 2.2rem;
}
.containerOne .footIcon {
    width: .9rem;
    margin: 0 auto;
    padding-top: .25rem;
}
.containerOne .footLeft h1 {
    color: #999;
    font-size: .55rem;
    text-align: center;
    line-height: 1rem;
}
.containerOne .footRight {
    background: #22cccc;
    width: 70%;
    line-height: 2.2rem;
    text-align: center;
    color: #fff;
    font-size: .9rem;
    height: 2.2rem;
}
.containerOne .contLast {
    margin-bottom: 5rem;
}